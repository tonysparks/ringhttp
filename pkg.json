{
    "name" : "ring",
    "version" : "0.0.1",
    "type" : "executable",
    "repo" : "",

    "build_command" : {
        "default" : {
            "default" : {
                "default" : {
                    "cc" : "gcc",
                    "cc_options" : "-g -std=gnu99 %input% -o %output% -D_CRT_SECURE_NO_WARNINGS",
                    "lita_options" : [
                        "-verbose"
                    ]
                }
            }
        },
        "release" : {
            "default" : {
                "default" : {
                    "cc" : "gcc",
                    "cc_options": [
                        "-std=gnu99",
                        "-O3",
                        "-march=native",
                        "-mtune=native",
                        "%input%",
                        "-o",
                        "%output%",
                        "-D_CRT_SECURE_NO_WARNINGS"
                    ],
                    "lita_options" : [
                        "-verbose"
                    ]
                }
            }
        }
    },

    "dependencies": [
        {
            "name" : "liburing",
            "version" : "v0.0.3",
            "repo": "file:///home/tony/projects/liburing"
            //"repo" : "https://github.com/tonysparks/liburing"
        }
    ],

}
